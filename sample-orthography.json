{
  "rules": [
    {
      "pattern": "^(.*)e \\^ed$",
      "replacement": "\\1ed"
    },
    {
      "pattern": "^(.*)s \\^s$",
      "replacement": "\\1les"
    }
  ],
  "aliases": [
    {
      "suffix": "age",
      "alias": "edge"
    },
    {
      "suffix": "ful",
      "alias": "fill"
    }
  ],
  "auto-suffix": [
    {
      "key": "-Z",
      "suffix": "{^s}"
    },
    {
      "key": "-D",
      "suffix": "{^ed}"
    },
    {
      "key": "-S",
      "suffix": "{^s}"
    },
    {
      "key": "-G",
      "suffix": "{^ing}"
    }
  ]
}
